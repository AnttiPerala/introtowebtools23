{
    "manifest_version": 3,
    "name": "Simple Autofill",
    "version": "1.0",
    "description": "A simple auto-fill extension",
    "permissions": [
        "activeTab",
        "scripting",
        "contextMenus",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "48": "icons/icon.png"
        }
    },
    "icons": {
        "48": "icons/icon.png"
    }
}